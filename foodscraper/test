    // Keep separate from select a store input functions
	function lpwdFocus() {
		$('#layerfakepassword').hide();
		$('#passwordField').show();
		$('#passwordField').focus();
	}
	function lpwdBlur() {
		if ($('#passwordField').attr('value') == '') {
			$('#passwordField').hide();
			$('#layerfakepassword').show();
		}
	}
    function userFocus() {
		$('#layerfakeuser').hide();
		$('.loginlayeruserName').show();
		$('.loginlayeruserName').focus();
	}
	function userBlur() {
		if ($('.loginlayeruserName').attr('value') == '') {
			 $('.loginlayeruserName').hide();
			 $('#layerfakeuser').show();
		}
	}

	
	$("#checkoutArea").hide();

	
