                <script type="text/javascript">
               $(function(){
                   // Adjust search input
                   var updateSearchWidth = (function(){
                      var newBtnWidth = $('.searchAll').width();
                      var textBoxWidth = 388 - 42 - newBtnWidth - 20 + 'px';
                      $('input#keyword').css('width',textBoxWidth);
                   });

                   // Keep search button updated on new page
                   var selectOption = (function(){
                        var selectedOption;
                        var $productTypeId = $('#productTypeId').val();
                        switch ($productTypeId){
                             case "P":
                                 $('.productSearch').addClass('selectedSearch');
                                 selectedOption = "Products";
                                 break;
                             case "R":
                                 $('.recipeSearch').addClass('selectedSearch');
                                 selectedOption = "Recipes";
                                 break;
                             case "EC":
                                 $('.articleSearch').addClass('selectedSearch');
                                 selectedOption = "Articles";
                                 break;

                             default:
                                 $('.allSearch').addClass('selectedSearch');
                                 selectedOption = "All";
                                 break;
                          }
                         $(".searchAllInner").text(selectedOption);
                         // alert('selectedOption is: ' + selectedOption + ' and $productTypeId is: ' + $productTypeId);
                         updateSearchWidth();
                   });
                   selectOption();

                   // Search type button
                  $('.searchList a').click(function(evt){
                      evt.preventDefault();
                      $('.searchList a').removeClass('selectedSearch');
                      $(this).addClass('selectedSearch');
                      searchBtnText = $(this).text();
                      $('#productTypeId').val($(this).attr("hreflang"));
                      $(".searchAllInner").text(searchBtnText);
                      updateSearchWidth();
                  });
                });

           </script>
