	function doFtrEmailSignUpFocus(component) {
	    var searchVal = component.value;
	    if (searchVal == "Your Email Address") {
	        component.value = "";
	    }
	}
	function doFtrEmailSignUpBlur(component) {
	    var searchVal = component.value;
	    if (strTrim(searchVal) == '') {
	        component.value = "Your Email Address";;
	    }
	}

</script>
